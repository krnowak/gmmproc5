noinst_LIBRARIES += gmmproc/xml/structured/libstructuredxml.a

gmmproc_xml_structured_libstructuredxml_a_SOURCES = \
  gmmproc/xml/structured/exceptions.cc \
  gmmproc/xml/structured/generic-attr.cc \
  gmmproc/xml/structured/generic-child.cc

gmmproc_xml_structured_libstructuredxml_a_CPPFLAGS = $(common_cppflags) $(gmmproc_xml_cppflags)
gmmproc_xml_structured_libstructuredxml_a_CXXFLAGS = $(common_cxxflags)

# for other subprojects using the libstructuredxml.a library
gmmproc_xml_structured_cppflags = $(gmmproc_xml_cppflags)
gmmproc_xml_structured_ldadd = \
  $(gmmproc_xml_ldadd) \
  $(top_builddir)/gmmproc/xml/structured/libstructuredxml.a
